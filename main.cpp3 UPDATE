#include <iostream>
#include <windows.h>
using namespace std;
int main() {
	setlocale(0,"");
	
	int number;
	system("cls");
	
		cout << "[+] Введите число:";

		cin >> number;


		if (number < 0) {
			cout << "[+]Ошибка! Число меньше 0" << endl;
		}else if (number > 100) { cout << "[+]Ошибка! Число больше 100" << endl; }
		
		else if (number <= 10) { cout << "[+] Цифра в диапазоне: 0-10" << endl; }
		else if (number <= 21) { cout << "[+] Цифра в диапазоне: 11-20" << endl; }
		else if (number <= 31) { cout << "[+] Цифра в диапазоне: 21-30" << endl; }
		else if (number <= 40) { cout << "[+] Цифра в диапазоне: 31-40" << endl; }
		else if (number <= 50) { cout << "[+] Цифра в диапазоне: 41-50" << endl; }
		else if (number <= 60) { cout << "[+] Цифра в диапазоне: 51-60" << endl; }
		else if (number <= 70) { cout << "[+] Цифра в диапазоне: 61-70" << endl; }
		else if (number <= 80) { cout << "[+] Цифра в диапазоне: 71-80" << endl; }
		else if (number <= 90) { cout << "[+] Цифра в диапазоне: 81-90" << endl; }
		else if (number <= 100) { cout << "[+] Цифра в диапазоне: 91-100" << endl; }
		else { cout << "[+]Неправильный ввод!" << endl; }


	
	int _;
	cin >> _;
return 0;
}
